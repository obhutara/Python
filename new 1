FROM jbemployee

--Question 6:
SELECT name,supplier
FROM jbitem
WHERE IN (SELECT supplier FROM jbsupplier WHERE supplier=89;);

--Question 9:
SELECT name
FROM jbcity
WHERE IN (SELECT city FROM jbsupplier;);

--Question 10:
SELECT name,color
FROM jbparts
WHERE weight > (SELECT weight FROM jbparts WHERE name="card reader");

--Quesiton 11:
SELECT name,color
FROM jbparts
ORDER BY weight DESC
LIMIT 4

12) What is the average weight of black parts?
--Quesiton 12:
SELECT name,color,weight
FROM jbparts
GROUP BY color
WHERE color="black"
